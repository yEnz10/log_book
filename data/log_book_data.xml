<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for log.book -->
        <record id="seq_log_book" model="ir.sequence">
            <field name="name">Log Book Number</field>
            <field name="code">log.book</field>
            <field name="prefix">LOG</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_log_book_hiv" model="ir.sequence">
            <field name="name">Form HIV Number</field>
            <field name="code">log.book.hiv</field>
            <field name="prefix">HIV</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>